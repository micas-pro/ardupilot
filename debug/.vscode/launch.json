{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Launch gpc_debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/gpc_debug",
        "args": [
          "/home/michal/git/PW/PN/Elka1Q/logs/gpc_debug_cpp_input.csv"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ],
        "miDebuggerPath": "/usr/bin/gdb"
      },
      {
        "name": "CMAKE (gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        // Resolved by CMake Tools:
        "program": "${command:cmake.launchTargetPath}",
        "args": [
          "/home/michal/git/PW/PN/Elka1Q/logs/gpc_debug_cpp_input.csv"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [
            {
                // add the directory where our target was built to the PATHs
                // it gets resolved by CMake Tools:
                "name": "PATH",
                "value": "$PATH:${command:cmake.launchTargetDirectory}"
            },
            {
                "name": "OTHER_VALUE",
                "value": "Something something"
            }
        ],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
        ,
        "miDebuggerPath": "/usr/bin/gdb"
    }
    ]
  }